{
  "name": "plist",
  "description": "Mac OS X Plist parser/builder for Node.js and browsers",
  "version": "0.4.3",
  "author": "Nathan Rajlich <nathan@tootallnate.net>",
  "contributors": [
    "Hans Huebner <hans.huebner@gmail.com>",
    "Pierre Metrailler",
    "Mike Reinstein <reinstein.mike@gmail.com>",
    "Vladimir Tsvang",
    "Mathieu D'Amours"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/TooTallNate/node-plist.git"
  },
  "keywords": [
    "apple",
    "mac",
    "plist",
    "parser",
    "xml"
  ],
  "main": "lib/plist.js",
  "dependencies": {
    "base64-js": "0.0.6",
    "xmlbuilder": "2.2.1",
    "xmldom": "0.1.x",
    "util-deprecate": "1.0.0"
  },
  "devDependencies": {
    "mocha": "1.18.2",
    "browserify": "3.44.2"
  },
  "scripts": {
    "test": "mocha -gc --reporter spec test/*.js"
  },
  "testling": {
    "browsers": [
        "ie9", "ie10", "ie11",
        "firefox/15",
        "chrome/22",
        "opera/12",
        "safari/5.1"
    ],
    "harness" : "mocha",
    "files": "test/*.js"
  }
}
